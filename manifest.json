{
  "name": "Spoiler Blocker",
  "version": "0.2",
  "description": "A chrome extention that blocks spoilers of your favourite movies",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}